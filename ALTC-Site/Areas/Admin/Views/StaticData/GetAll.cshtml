@model IEnumerable<ALTC_WebSite.Models.StaticData>

@{
    ViewData["Title"] = "GetAll";
}

<h1>GetAll</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.aboutus)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <div style=" margin-left: 403px;">
                <div style="margin-bottom:-24px;  margin-left: 72px; ">
               @Html.ActionLink("Delete", "Delete", new { id = item.id })

               </div>

                @Html.ActionLink("Edit", "Edit", new { id = item.id })

        </div>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
                <td>
                    @* @Html.DisplayFor(modelItem => item.aboutus) *@
                    @Html.Raw(item.aboutus)
                </td>
          
        </tr>
}
    </tbody>
</table>
